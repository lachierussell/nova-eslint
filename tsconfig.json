{
  "compilerOptions": {
    "target": "ESNext",
	"moduleResolution": "ES2020",
    "sourceMap": true,
    "noEmit": true,
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
	"allowSyntheticDefaultImports": true
  }
}
